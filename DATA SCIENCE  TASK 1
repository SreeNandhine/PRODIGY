import matplotlib.pyplot as plt

# Sample data representing age groups and their frequencies
age_groups = ['0-9', '10-19', '20-29', '30-39', '40-49', '50-59', '60-69', '70-79', '80-89', '90+']
frequencies = [100, 200, 500, 800, 700, 600, 400, 300, 200, 100]

plt.figure(figsize=(10, 6))
plt.bar(age_groups, frequencies, color='skyblue')
plt.xlabel('Age Groups')
plt.ylabel('Frequency')
plt.title('Distribution of Ages in a Population')
plt.xticks(rotation=45)
plt.tight_layout()
plt.show()
